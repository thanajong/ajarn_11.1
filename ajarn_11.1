#include<stdio.h>
int main()
{
	float a,b,c;
	float x,y,z;
	scanf("%f %f %f",&a,&b,&c);
	if (a<=b&&a<=c)
	{
		x=a;
		y=b;
		z=c;
	}
	else if (b<=a&&b<=c)
	{
		x=b;
		y=a;
		z=c;
	}
	else if (c<=a&&c<=b)
	{
		x=c;
		y=a;
		z=b;
	}
	
	if(y<=z)
	{
		printf("%f", x+y);
	}
	else if(z<=y)
	{
		printf("%f", x+z);
	}
	return 0;
}
